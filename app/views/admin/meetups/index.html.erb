<%= form_for [:admin, @meetup], :validate => true do |f| %>

  <% if @meetup.errors.any? %>
    <% @meetup.errors.full_messages.each do |msg| %>
      <li class="form-errors"><%= msg %><li>
    <% end %>
  <% end %>

  <%= f.label :meeting_url %>
  <%= f.text_field :meeting_url %>

  <br />

  <%= f.label :project_night_url %>
  <%= f.text_field :project_night_url %>

  <br />

  <%= f.submit 'Save' %>

 <%- end -%>
